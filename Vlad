<script>
var imgs=['1000.webp','1001.webp','1002.webp','1003.webp','1004.webp','1005.webp','1006.webp'];
var text=['–∫–æ—Ç1', '–∫–æ—Ç2', '–∫–æ—Ç3', '–∫–æ—Ç4', '–∫–æ—Ç5','–∫–æ—Ç6','–∫–æ—Ç7'];
var now=-1; 

function r_click() 
{ 
 var obj = document.getElementById("img");
 
   if (now < imgs.length-1)  
   now++ 
   else
     now = 0;
     obj.src = imgs[now];	
	printt.textContent = text[now];	 
}

function l_click() 
{     
 var obj = document.getElementById("img");
if (now > 0) now--;
  else
    now = imgs.length-1;
    obj.src = imgs[now];	
	printt.textContent = text[now];	 	
}
</script>
